// XML运算
module.exports = {
  // MARK 价格处理
  toPrice: function (value) {
    return parseFloat(value).toFixed(2)
  },
  // MARK 获取url
  toUrl: function (value) {
    //转字符串
    var url = value + ''
    if (url.indexOf('http') > -1) {
      return url
    }

    var baseUrl = 'http://legong.frnnet.com'
    return baseUrl + value
  },

  toModel: function (value) {
    return value.substring(3)
  },
}
