.cell {
  background: white;
  height: 120px;
  border-bottom: 10px solid rgb(245, 245, 245);
  align-items: center;
  padding: 0 10px;
  font-size: 10px;
}

.cell .head {
  display: flex;
  justify-content: space-between;
  width: 100%;
  line-height: 30px;
}

.head .time {
  color: red;
}

/* 内容 */
.cell .content {
  display: flex;
  font-size: 12px;
}

.content .other {
  margin-left: 10px;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  position: relative;
}

.content .image {
  width: 80px;
  height: 80px;
}

.content .price {
  color: red;
  font-size: 14px;
}

.content-bottom {
  margin-top: 5px;
  display: flex;
  font-size: 10px;
  justify-content: space-between;
}

.content-bottom .content-progress {
  width: 140px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.content-bottom .progress {
  flex: 1;
  margin-left: 25px;
  height: 5px;
}

.other .qg-btn {
  position: absolute;
  top: 30px;
  right: 0;
  background: red;
  width: 60px;
  line-height: 20px;
  color: white;
  text-align: center;
  border-radius: 5px;
  font-size: 10px;
}
