.name {
  line-height: 50px;
  font-size: 12px;
  background: white;
  border-bottom: 1px solid rgb(245, 245, 245);
  padding: 0 10px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.name .avator {
  width: 40px;
  height: 40px;
  border-radius: 20px;
}

.name .content {
  margin-left: 10px;
  flex: 1;
  text-align: right;
  color: rgb(143, 143, 143);
}
